<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="coedil99.persistentmodel.CatalogoFornitore" table="CatalogoFornitore" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="name" column="Name" type="string" length="255" not-null="false" lazy="false"/>
		<list name="ORM_ProductDescription" lazy="true" cascade="save-update,lock" inverse="false">
			<key column="CatalogoFornitoreID" not-null="true"/>
			<index column="CatalogoFornitoreIndex" type="integer"/>
			<one-to-many class="coedil99.persistentmodel.ProductDescription"/>
		</list>
	</class>
</hibernate-mapping>
