<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="coedil99.persistentmodel.Dipendente" table="Dipendente" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="username" column="Username" type="string" length="255" not-null="false" lazy="false"/>
		<property name="pwd" column="Pwd" type="string" length="255" not-null="false" lazy="false"/>
		<property name="level" column="Level" type="integer" length="11" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
