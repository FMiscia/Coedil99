<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="coedil99.persistentmodel.DocumentoOttimizzazione" table="DocumentoOttimizzazione" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<list name="ORM_Items" lazy="true" cascade="save-update,lock" inverse="false">
			<key column="DocumentoOttimizzazioneID" not-null="false"/>
			<index column="DocumentoOttimizzazioneIndex" type="integer"/>
			<one-to-many class="coedil99.persistentmodel.Item"/>
		</list>
	</class>
</hibernate-mapping>
