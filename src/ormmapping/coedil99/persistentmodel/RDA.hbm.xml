<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="coedil99.persistentmodel.RDA" table="RDA" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="state" column="State" type="string" length="255" not-null="false" lazy="false"/>
		<property name="date" column="`Date`" type="date" not-null="false" lazy="false"/>
		<property name="descrizione" column="Descrizione" type="string" length="255" not-null="false" lazy="false"/>
		<list name="ORM_RigheRDA" lazy="true" cascade="save-update,lock" inverse="false">
			<key column="RDAID" not-null="true"/>
			<index column="RDAIndex" type="integer"/>
			<one-to-many class="coedil99.persistentmodel.RigaRDA"/>
		</list>
	</class>
</hibernate-mapping>
