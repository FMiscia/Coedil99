<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="coedil99.persistentmodel.Ordine" table="Ordine" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="cliente" cascade="lock" column="ClienteID" class="coedil99.persistentmodel.Cliente" not-null="true" update="false" insert="false" lazy="no-proxy" access="field">
		</many-to-one>
		<property name="ordineGestionale" column="OrdineGestionale" type="string" length="255" not-null="false" lazy="false"/>
		<property name="dataInizio" column="DataInizio" type="date" not-null="false" lazy="false"/>
		<property name="dataScadenza" column="DataScadenza" type="date" not-null="false" lazy="false"/>
		<property name="dataFine" column="DataFine" type="date" not-null="false" lazy="false"/>
		<property name="anno" column="Anno" type="integer" length="11" not-null="false" lazy="false"/>
		<property name="numeroOrdine" column="NumeroOrdine" type="integer" length="11" not-null="false" lazy="false"/>
		<property name="OC" column="OC" type="string" length="255" not-null="false" lazy="false"/>
		<list name="ORM_Commesse" lazy="true" cascade="save-update,lock" inverse="false">
			<key column="OrdineID" not-null="true"/>
			<index column="OrdineIndex" type="integer"/>
			<one-to-many class="coedil99.persistentmodel.Commessa"/>
		</list>
	</class>
</hibernate-mapping>
