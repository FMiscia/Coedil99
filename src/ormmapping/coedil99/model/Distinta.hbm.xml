<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="coedil99.model.Distinta" table="Distinta" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="ddo" column="DocumentoOttimizzazioneID" class="coedil99.model.DocumentoOttimizzazione" cascade="save-update,lock" unique="true" not-null="false" lazy="no-proxy" access="field">
		</many-to-one>
		<list name="ORM_Lavori" lazy="true" cascade="save-update,lock" inverse="false">
			<key column="DistintaID" not-null="false"/>
			<index column="DistintaIndex" type="integer"/>
			<one-to-many class="coedil99.model.RigaLavoro"/>
		</list>
	</class>
</hibernate-mapping>
