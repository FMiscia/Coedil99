<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="coedil99.model.ProductDescription" table="productdescription" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="catalogoFornitore" cascade="lock" column="CatalogoFornitoreID" class="coedil99.model.CatalogoFornitore" not-null="true" update="false" insert="false" lazy="no-proxy" access="field">
		</many-to-one>
	</class>
</hibernate-mapping>
