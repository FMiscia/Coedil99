<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="coedil99.model.ProductDescription" table="ProductDescription" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="catalogoFornitore" cascade="lock" column="CatalogoFornitoreID" class="coedil99.model.CatalogoFornitore" not-null="true" update="false" insert="false" lazy="no-proxy" access="field">
		</many-to-one>
		<property name="essenza" column="Essenza" type="string" length="255" not-null="false" lazy="false"/>
		<property name="prezzo" column="Prezzo" type="float" not-null="false" lazy="false"/>
		<property name="pezzi_per_pacco" column="Pezzi_per_pacco" type="integer" length="11" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
